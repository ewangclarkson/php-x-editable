{
    "name": "diana/php-x-editable",
    "description": "Maybe the best x-editable plugin for PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {"name": "xiaohui lam", "email": "xiaohui.lam@e.hexdata.cn"}
    ],
    "require": {
        "php": ">5.3",
        "psr/http-message": "1.*",
        "avplab/php-html-builder": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Editable\\": "src/",
            "Editable\\Example\\": "example/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Editable\\Test\\": "test/"
        }
    }
}
